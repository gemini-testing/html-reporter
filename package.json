    "rc-resize-observer": "^1.0.0",
    "react": "^16.13.1",
    "react-checkbox-tree": "^1.8.0",
    "react-clipboard.js": "^1.1.2",
    "react-dom": "^16.13.1",
    "react-hotkeys": "^2.0.0",
    "react-html-parser": "^2.0.2",
    "react-markdown": "^3.6.0",
    "react-redux": "^7.2.1",
    "react-virtualized": "^9.22.3",
    "reapop": "4.0.5",
