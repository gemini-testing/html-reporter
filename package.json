{
  "name": "html-reporter",
  "version": "5.3.2",
  "description": "Plugin for hermione which is intended to aggregate the results of tests running into html report",
  "scripts": {
    "coverage": "nyc npm run test-unit",
    "lint": "eslint .",
    "test": "npm run lint && npm run test-unit",
    "test-unit": "cross-env NODE_ENV=test mocha test/unit",
    "test-func": "npm run make-fixtures; cross-env NODE_ENV=test hermione",
    "make-fixtures": "npm run clean-fixtures && cross-env NODE_ENV=test hermione -c test/func/fixtures/.hermione.fixtures.js",
    "make-fixtures-gui": "npm run clean-fixtures && cross-env NODE_ENV=test hermione gui -c test/func/fixtures/.hermione.fixtures.js",
    "clean-fixtures": "rimraf test/func/fixtures/report",
    "precommit": "npm run lint",
    "prepublish": "cross-env NODE_ENV=production webpack --config=webpack.prod.js",
    "start": "webpack-dev-server --config=webpack.dev.js --progress --colors --hot --inline",
    "commitmsg": "conventional-changelog-lint -e",
    "release": "standard-version",
    "watch": "webpack --progress -d --config=webpack.dev.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gemini-testing/html-reporter.git"
  },
  "bugs": {
    "url": "https://github.com/gemini-testing/html-reporter/issues"
  },
  "engines": {
    "node": ">= 8"
  },
  "keywords": [
    "hermione",
    "plugin",
    "html-reporter"
  ],
  "homepage": "https://github.com/gemini-testing/html-reporter#readme",
  "license": "MIT",
  "peerDependencies": {
    "hermione": ">=2.7.0"
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.2",
    "chalk": "^1.1.3",
    "debug": "^4.1.1",
    "express": "^4.16.2",
    "fs-extra": "^7.0.1",
    "gemini-configparser": "^1.0.0",
    "gemini-core": "^3.7.0",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.4",
    "looks-same": "^7.1.1",
    "nested-error-stacks": "^2.1.0",
    "opener": "^1.4.3",
    "p-queue": "^5.0.0",
    "react-markdown": "^3.2.0",
    "resolve": "^1.5.0",
    "semver": "^5.5.0",
    "shelljs": "^0.8.1",
    "signal-exit": "^3.0.2",
    "@gemini-testing/sql.js": "^1.0.1",
    "sqlite3": "^4.1.0",
    "tmp": "^0.1.0",
    "urijs": "^1.18.12"
  },
  "devDependencies": {
    "@gemini-testing/react-lazily-render": "^1.2.0",
    "@gemini-testing/react-lazyload": "^2.3.2",
    "app-module-path": "^2.2.0",
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "classnames": "^2.2.5",
    "conventional-changelog-lint": "^1.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.8.0",
    "eslint-config-gemini-testing": "^2.8.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "hermione": "^2.6.1",
    "hermione-global-hook": "^1.0.1",
    "hermione-headless-chrome": "0.0.2",
    "html-react-parser": "^0.4.0",
    "html-reporter-test-server": "file:test/func/plugins/html-reporter-test-server",
    "html-reporter-tester": "file:test/func/plugins/html-reporter-tester",
    "html-webpack-include-assets-plugin": "^1.0.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.11.9",
    "ignore-styles": "^5.0.1",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "mocha": "^3.0.2",
    "nyc": "^14.1.1",
    "prop-types": "^15.6.0",
    "proxyquire": "^1.7.11",
    "qemitter": "^1.0.0",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-clipboard.js": "^1.1.2",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.83.0",
    "sinon": "^4.0.1",
    "standard-version": "^4.0.0",
    "style-loader": "^0.19.0",
    "styled-components": "^3.4.10",
    "url-loader": "^1.1.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.3",
    "webpack-merge": "^4.1.1"
  }
}
