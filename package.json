{
  "name": "html-reporter",
  "version": "9.7.0",
  "description": "Plugin for hermione which is intended to aggregate the results of tests running into html report",
  "scripts": {
    "coverage": "nyc npm run test-unit",
    "lint": "eslint .",
    "test": "npm run lint && npm run test-unit",
    "test-unit": "cross-env NODE_ENV=test mocha test/unit",
    "test-func": "npm run test-func-main && npm run test-func-plugins",
    "test-func-main": "npm run make-main-fixtures; cross-env NODE_ENV=test hermione",
    "test-func-plugins": "npm run make-plugins-fixtures; cross-env NODE_ENV=test hermione -c test/func/plugins/.hermione.plugins.js",
    "make-main-fixtures": "npm run clean-fixtures && cross-env NODE_ENV=test hermione -c test/func/fixtures/.hermione.fixtures.js",
    "make-plugins-fixtures": "npm run clean-fixtures && cross-env NODE_ENV=test html_reporter_plugins_enabled=true hermione -c test/func/fixtures/.hermione.fixtures.js",
    "make-main-fixtures-gui": "npm run make-main-fixtures -- gui",
    "make-plugins-fixtures-gui": "npm run make-plugins-fixtures -- gui",
    "clean-fixtures": "rimraf test/func/fixtures/report",
    "precommit": "npm run lint",
    "prepublish": "cross-env NODE_ENV=production webpack build --config=webpack.prod.js --progress",
    "start": "NODE_ENV=development webpack serve --config=webpack.dev.js --progress",
    "commitmsg": "conventional-changelog-lint -e",
    "release": "standard-version",
    "watch": "NODE_ENV=development webpack watch --config=webpack.dev.js --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gemini-testing/html-reporter.git"
  },
  "bugs": {
    "url": "https://github.com/gemini-testing/html-reporter/issues"
  },
  "engines": {
    "node": ">= 12"
  },
  "keywords": [
    "hermione",
    "plugin",
    "html-reporter"
  ],
  "homepage": "https://github.com/gemini-testing/html-reporter#readme",
  "license": "MIT",
  "peerDependencies": {
    "hermione": ">=2.7.0"
  },
  "dependencies": {
    "@gemini-testing/sql.js": "^1.0.1",
    "axios": "^0.18.1",
    "better-sqlite3": "^7.4.4",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.2",
    "chalk": "^1.1.3",
    "debug": "^4.1.1",
    "eventemitter2": "6.4.7",
    "express": "^4.16.2",
    "filesize": "^8.0.6",
    "fs-extra": "^7.0.1",
    "gemini-configparser": "^1.0.0",
    "http-codes": "1.0.0",
    "inquirer": "^8.2.0",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.4",
    "looks-same": "^8.1.0",
    "nested-error-stacks": "^2.1.0",
    "opener": "^1.4.3",
    "ora": "^5.4.1",
    "p-queue": "^5.0.0",
    "qs": "^6.9.1",
    "signal-exit": "^3.0.2",
    "tmp": "^0.1.0",
    "urijs": "^1.18.12"
  },
  "devDependencies": {
    "@gemini-testing/commander": "^2.15.3",
    "app-module-path": "^2.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
