    "css-loader": "^0.28.7",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint": "^4.8.0",
    "eslint-config-gemini-testing": "^2.8.0",
    "eslint-plugin-babel": "^5.3.1",
