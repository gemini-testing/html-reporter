{
    "extends": "../tsconfig.common.json",
    "include": ["index.ts", "../lib/static/new-ui/types/typings.d.ts"],
    "exclude": ["build"],
    "compilerOptions": {
        "outDir": "./build",
        "declaration": true,
        "declarationMap": true,
        "module": "commonjs",
        "target": "es2015",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "noUnusedLocals": false
    }
}
